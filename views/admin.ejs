    <div class="site">
        
        <br>
        <br>
        <br>
        <div class="ui container">
            <form id="form" action="/add" class="ui form" method="POST">
                <div class="three fields">
                    <div class="field">
                        <label for="label" class="ui header" style="color: white;">Enter Company Name</label>
                        <input type="text" name="name" placeholder="Enter Company Name" value=""  id="name">
                    </div>
                    <div class="field">
                        <label for="label" class="ui header" style="color: white;">Enter Description</label>
                        <!-- <input type="text" name="players" placeholder="Enter Description" value="" id="players"> -->
                        <textarea type="text" name="players" placeholder="Enter Description" value="" id="players" rows="4" cols="50"></textarea>
                            
                    </div>
                    <div class="field">
                        <label for="label" class="ui header " style="color: white;">Enter Form Link</label>
                        <input type="text" name="coach" placeholder="Enter Form Link" value="" id="coach">
                    </div>
                </div>
                <!-- <div class=" one fields"> -->
                    <div class="field fluid">
                        <button type="submit" class="ui button fluid blue">Save</button>
                    </div>
                <!-- </div> -->
            </form>
            <br>
            <br>
            <br>
            
            <table class="ui table blue" >
                <thead>
                    <tr>
                        <th>Recruiter</th>
                        <th>Players</th>
                        <th>Link</th>
                        <th >Edit</th>
                        <th>Delete</th>
                    </tr>
                </thead>
                <tbody>
                    <% teams.forEach((team)=>{%>
                        <tr>
                            <td id="teamName"><%- team.name %></td>
                            <td id="teamPlayers"><%- team.players %></td>
                            <td> <a href="<%- team.coach %>"><%- team.coach %></a> </td>

                            <!-- <td id="teamCoach"><%- team.coach %></td> -->
                            <td><a class="ui button blue editBtn" href="/edit/<%= team._id %>"><i class="edit white icon"></i></a></td>
                            <td><a class="ui button blue actnBtn" href="/<%= team._id %>"><i id="delete" class="minus red icon"></i></a></td>
                        </tr>
                      <% }); %>
                </tbody>
            
